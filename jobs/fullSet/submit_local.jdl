Error =  /nfs_scratch/emettner/jobs/fullSet/$(myLogFolder)/batch_$(cluster)_$(process).stderr
Log =    /nfs_scratch/emettner/jobs/fullSet/$(myLogFolder)/batch_$(cluster)_$(process).condor
